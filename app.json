{
  "expo": {
    "name": "projectturtle",
    "slug": "projectturtle",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jeremydain.turtle"
    },
    "android": {},
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#f5f5f5",
      "hide": true
    },
    "androidNavigationBar": {
      "visible": "leanback",
      "barStyle": "light-content",
      "backgroundColor": "#f5f5f5"
    },
    "description": ""
  }
}
